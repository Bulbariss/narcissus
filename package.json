{
  "name": "gatsby-starter",
  "description": "Gatsby Starter",
  "version": "1.0.0",
  "author": "Me",
  "dependencies": {
    "gatsby": "2.24.56",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.17",
    "gatsby-plugin-manifest": "2.4.28",
    "gatsby-plugin-offline": "3.2.27",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-purgecss": "5.0.0",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.34",
    "gatsby-plugin-styled-jsx": "^3.3.11",
    "gatsby-source-filesystem": "^2.3.29",
    "gatsby-transformer-sharp": "^2.5.14",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-device-detect": "^1.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-player": "^2.6.2"
  },
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop2": "gatsby develop -H 0.0.0.0",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "styled-jsx": "^3.3.0",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.8.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-react": "7.20.6",
    "gatsby-plugin-eslint": "2.0.8",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react-hooks": "^4.1.0",
    "prettier": "2.1.1",
    "tailwindcss": "1.8.6"
  }
}
